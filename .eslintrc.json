  {
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console":"off",
        "prettier/prettier": ["error"],
        "no-undef":"off",
        "func-names": "off"
    },
    "overrides": [
      {
        "files": ["tests/**/*"],
        "env": {
          "jest": true
        }
      }
    ]
    
    
  }